    <Style x:Key="DownArrowPath" TargetType="{x:Type Path}">
        <Setter Property="Fill" Value="{StaticResource Black}"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Data" Value="M 0 0 L 4 4 L 8 0 Z"/>
        <Setter Property="Stroke" Value="Transparent"/>
        <Setter Property="Width" Value="8"/>
    </Style>

    <Style x:Key="UpArrowPath" TargetType="{x:Type Path}" BasedOn="{StaticResource DownArrowPath}">
        <Setter Property="RenderTransform">
            <Setter.Value>
                <RotateTransform CenterX="4" CenterY="4" Angle="180"/>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="LeftArrowPath" TargetType="{x:Type Path}" BasedOn="{StaticResource DownArrowPath}">
        <Setter Property="RenderTransform">
            <Setter.Value>
                <RotateTransform CenterX="4" CenterY="4" Angle="90"/>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="RightArrowPath" TargetType="{x:Type Path}" BasedOn="{StaticResource DownArrowPath}">
        <Setter Property="RenderTransform">
            <Setter.Value>
                <RotateTransform CenterX="4" CenterY="4" Angle="-90"/>
            </Setter.Value>
        </Setter>
    </Style>
