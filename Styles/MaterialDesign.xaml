        <ResourceDictionary Source="pack://application:,,,/Styles;component/Dictionaries/ButtonsDictionary.xaml"/>
        <ResourceDictionary Source="pack://application:,,,/Styles;component/Dictionaries/ControlsDictionary.xaml"/>

    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>